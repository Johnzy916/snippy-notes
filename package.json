{
  "name": "snippy-notes",
  "version": "1.0.0",
  "description": "A simple notes app. Take notes, save, and expand snippets with ease.",
  "license": "MIT",
  "scripts": {
    "build": "parcel build index.html --public-url ./",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --config=jest.config.json --runInBand --watchAll",
    "start": "parcel index.html --open"
  },
  "dependencies": {
    "dompurify": "^2.0.11",
    "dotenv": "^8.2.0",
    "firebase": "^7.14.6",
    "history": "^4.10.1",
    "normalize.css": "8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-alert": "^7.0.0",
    "react-dom": "^16.13.1",
    "react-dropdown": "^1.7.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.4.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "firestore-jest-mock": "^0.4.0",
    "jest": "^25.5.3",
    "parcel": "^2.4.0",
    "process": "^0.11.10",
    "raf": "^3.4.1",
    "redux-mock-store": "^1.5.4"
  },
  "sideEffects": false,
  "browserslist": [
    "> 1%",
    "not dead"
